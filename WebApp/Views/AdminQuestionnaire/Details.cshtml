@model Entities.Questionnaire

@{
    ViewData["Title"] = "Détails";
}

<h1>Détails Questionnaire</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Titre
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Title)
        </dd>
        <dt class="col-sm-2">
            Temps par défaut
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DefaultTimer)
        </dd>
        <dt class="col-sm-2">
            Utilisateur
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.FirstName) @Html.DisplayFor(model => model.User.LastName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Retour à la liste</a>
</div>